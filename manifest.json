//Contain properties of extension
{
	"manifest_version": 2,

	"name": "YeezyVision"
	"description":"Extension made for Yeezy's supporter fan club ",
	"version": "1.0",

	"browser_action": {
        "name":"YeezyVision"
        "icons":["icon.png"]
		"default_icon":"icon.png"
},
	"permissions": [
		"activeTab",
		"tabs"
	],
	"content-scripts": [{
        "js": ["jquery.min.js", "background.js" ],
        "matches": ["http://*/*", "https://*/*"]
    }]
	}
}

